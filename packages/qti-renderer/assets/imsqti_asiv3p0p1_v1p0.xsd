            <xs:extension base="BaseSequenceXBaseDType">
                <xs:sequence>
                    <xs:choice minOccurs="0" maxOccurs="unbounded">
                        <xs:element ref="dt" minOccurs="1" maxOccurs="unbounded"/>
                        <xs:element ref="dd" minOccurs="1" maxOccurs="unbounded"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

