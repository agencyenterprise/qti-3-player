                    <xs:element ref="img" minOccurs="1" maxOccurs="1"/>
                    <xs:element ref="picture" minOccurs="1" maxOccurs="1"/>
                </xs:choice>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

