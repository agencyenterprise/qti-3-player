                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
        <xs:attribute name="aria-setsize" use="optional" type="xs:integer"/>
        <xs:attribute name="aria-sort" use="optional" default="none">
            <xs:simpleType>
                <xs:annotation>
                    <xs:documentation source="documentation">
